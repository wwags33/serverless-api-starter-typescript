{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node16",
    "noImplicitAny": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "ES2020",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", "**/*.test.ts"]
}
